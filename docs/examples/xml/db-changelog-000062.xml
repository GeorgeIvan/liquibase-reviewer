<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd"
  logicalFilePath="db-changelog-000062.xml">

  <changeSet author="appian" id="tag-000061">
    <tagDatabase tag="000061"/>
  </changeSet>

  <changeSet author="appian" id="000062.0.0">
    <comment>Create a updated_ts_for_sync column on tp_feed_entry, for storing the update time stamp used for sync across servers.</comment>
    <addColumn tableName="tp_feed_entry">
      <column name="updated_ts_for_sync" type="${longType}" defaultValue="0">
        <constraints nullable="false"/>
      </column>
    </addColumn>
  </changeSet>

</databaseChangeLog>
